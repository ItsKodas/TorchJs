  "scripts": {
    "dev": "npx ts-node -r tsconfig-paths/register index.ts",
    "build": "npm run webpack && npm run tailwind && npx tsc -p tsconfig.json && npx tscpaths -p tsconfig.json -s . -o ../dist/Server",
    "start": "cd ../dist/Server && node --experimental-specifier-resolution=node index.js",
    "webpack": "npx webpack",
    "tailwind": "npx postcss public/css/tailwind.css -o public/css/styles.css"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "discord.js": "^14.6.0",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "formidable": "^2.0.1",
    "mongodb": "^4.11.0",
    "webpack": "^5.75.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@codemirror/autocomplete": "^6.3.4",
    "@codemirror/commands": "^6.1.2",
    "@codemirror/lang-javascript": "^6.1.1",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-xml": "^6.0.1",
    "@codemirror/state": "^6.1.4",
    "@codemirror/theme-one-dark": "^6.1.0",
    "@codemirror/view": "^6.6.0",
    "@types/electron": "^1.6.10",
    "@types/express": "^4.17.14",
    "@types/formidable": "^2.0.5",
    "@types/xml2js": "^0.4.11",
    "autoprefixer": "^10.4.13",
    "codemirror": "^6.0.1",
