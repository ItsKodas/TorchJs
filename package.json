{
  "name": "torchjs",
  "version": "2.0.0",
  "main": "main.js",
  "author": "Jessica Lancelot",
  "license": "ISC",
  "scripts": {
    "dev:client": "cd ./Client && npx ts-node -r tsconfig-paths/register index.ts",
    "dev:server": "cd ./Server && npx ts-node -r tsconfig-paths/register index.ts",
    "build": "npm run webpack && npm run build:client && npm run build:server && npx ts-node ./Util/package.ts",
    "webpack": "cd ./Server && npx webpack",
    "tailwind": "cd ./Server && npx postcss public/css/tailwind.css -o public/css/styles.css",
    "production:client": "cd dist/Client && node --experimental-specifier-resolution=node index.js",
    "production:server": "cd dist/Server && node --experimental-specifier-resolution=node index.js",
    "build:client": "npx tsc -p ./Client/tsconfig.json && npx tscpaths -p Client/tsconfig.json -s ./Client -o ./dist/Client",
    "build:server": "npx tsc -p ./Server/tsconfig.json && npx tscpaths -p Server/tsconfig.json -s ./Server -o ./dist/Server"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "discord.js": "^14.6.0",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "formidable": "^2.0.1",
    "mongodb": "^4.11.0",
    "webpack": "^5.75.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@codemirror/autocomplete": "^6.3.4",
    "@codemirror/commands": "^6.1.2",
    "@codemirror/lang-javascript": "^6.1.1",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-xml": "^6.0.1",
    "@codemirror/state": "^6.1.4",
    "@codemirror/theme-one-dark": "^6.1.0",
    "@codemirror/view": "^6.6.0",
    "@types/express": "^4.17.14",
    "@types/formidable": "^2.0.5",
    "@types/xml2js": "^0.4.11",
    "autoprefixer": "^10.4.13",
    "codemirror": "^6.0.1",
    "postcss": "^8.4.19",
    "postcss-cli": "^10.0.0",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.8.4",
    "webpack-cli": "^5.0.0"
  }
}
